<div class="lead__item fb-switcher-item-js">
	<div class="lead__layer--front">
		<div class="lead__position">@@position</div>
		<div class="lead__name">@@name</div>
		<div class="lead__img">
			<img src="@@srcImg" alt="@@name" />
		</div>
		<div class="lead__footer"><a href="#" class="fb-switcher-handler-js"><span></span></a></div>
	</div>
	<div class="lead__layer--back">
		<div class="lead__align">
			<div class="lead__schedule">
				<dl>
					<dt>Сб-Вс:</dt>
					<dd>@@scheduleWork</dd>
					<dt>Пн-Пт:</dt>
					<dd>@@scheduleWeekend</dd>
				</dl>
			</div>
		</div>
	</div>
	<div class="lead__footer"><a href="#" class="fb-switcher-handler-js" data-text="Часы приема" data-text-alt="Назад"><span>Часы приема</span></a></div>
</div>